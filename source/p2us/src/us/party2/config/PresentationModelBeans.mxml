<?xml version="1.0" encoding="utf-8"?>
<swiz:BeanProvider xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 xmlns:swiz="http://swiz.swizframework.org">
	<fx:Script>
		<![CDATA[
			import us.party2.presentation.MainPresentationModel;
			import us.party2.presentation.QueryPresentationModel;
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
		<swiz:Prototype id="mainPresentationModel" 
						type="{MainPresentationModel}" 
						singleton="true"/>
		
		<swiz:Prototype id="queryPresentationModel" 
						type="{QueryPresentationModel}" 
						singleton="true"/>
	</fx:Declarations>
	
</swiz:BeanProvider>
